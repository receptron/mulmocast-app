{
  "name": "mulmocast-app",
  "productName": "MulmoCast",
  "version": "1.0.11",
  "description": "MulmoCast",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "NODE_ENV=development electron-forge start",
    "rebuild:native": "npx electron-rebuild -f",
    "dev": "NODE_ENV=development vite --config vite.renderer.config.ts",
    "package": "npx puppeteer browsers install chrome --path=./chromium && electron-forge package",
    "make:local": "bash -c 'set -a && source .env && set +a && npx puppeteer browsers install chrome --path=./chromium && electron-forge make'",
    "make:ci": "npx puppeteer browsers install chrome --path=./chromium && electron-forge make",
    "make:ci:mac": "yarn run make:ci -- --platform=darwin --arch=arm64",
    "make:ci:win": "yarn run make:ci -- --platform=win32 --arch=x64",
    "publish:ci": "npx puppeteer browsers install chrome --path=./chromium && electron-forge publish",
    "publish:ci:mac": "yarn run publish:ci -- --platform=darwin --arch=arm64",
    "publish:ci:win": "yarn run publish:ci -- --platform=win32 --arch=x64",
    "publish:ci:win_json": "npx tsx batch/win_meta.ts",
    "format": "prettier --write '{scripts/**/*,test/*,src/**/*,*}.{js,ts,vue,json}'",
    "format:check": "prettier --check '{scripts/**/*,src/**/*,*}.{js,ts,vue,json}'",
    "dumpi18n": "npx tsx test/i18n-dump.ts",
    "lint": "yarn run dumpi18n && eslint --ext .js,.ts,.vue src test scripts",
    "lint:fix": "eslint --ext .js,.ts,.vue src scripts --fix",
    "lint:vue": "vue-tsc --noEmit --project tsconfig.vue.json",
    "test": "NODE_ENV=test npx tsx --test ./test/test_*.ts",
    "type": "yarn run lint && yarn run type-check",
    "type-check": "tsc --noEmit -p ./tsconfig.main.json && tsc --noEmit -p ./tsconfig.renderer.json",
    "type-check:main": "tsc --noEmit -p ./tsconfig.main.json",
    "type-check:renderer": "tsc --noEmit -p ./tsconfig.renderer.json",
    "i18n:check": "npx tsx scripts/check-i18n.ts",
    "i18n:translate": "npx tsx scripts/translate-i18n.ts"
  },
  "keywords": [],
  "author": {
    "name": "receptron team"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-rpm": "^7.11.1",
    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "@electron-forge/plugin-vite": "^7.11.1",
    "@electron-forge/publisher-s3": "^7.11.1",
    "@electron/fuses": "^2.0.0",
    "@eslint/js": "^10.0.1",
    "@google/generative-ai": "^0.24.1",
    "@intlify/eslint-plugin-vue-i18n": "^4.1.1",
    "@types/adm-zip": "^0.5.7",
    "@types/clone-deep": "^4.0.4",
    "@types/deep-equal": "^1.0.4",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^25.2.3",
