{
  "name": "mulmocast-app",
  "productName": "MulmoCast",
  "version": "1.0.0",
  "description": "MulmoCast",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "NODE_ENV=development electron-forge start",
    "rebuild:native": "npx electron-rebuild -f",
    "dev": "NODE_ENV=development vite --config vite.renderer.config.ts",
    "package": "npx puppeteer browsers install chrome && electron-forge package",
    "make:local": "bash -c 'set -a && source .env && set +a && npx puppeteer browsers install chrome && electron-forge make'",
    "make:ci": "npx puppeteer browsers install chrome && electron-forge make",
    "format": "prettier --write '{test/*,src/**/*,*}.{js,ts,vue,json}'",
    "format:check": "prettier --check '{src/**/*,*}.{js,ts,vue,json}'",
    "publish": "electron-forge publish",
    "dumpi18n": "npx tsx test/i18n-dump.ts",
    "lint": "yarn run dumpi18n && eslint --ext .js,.ts,.vue src test",
    "lint:fix": "eslint --ext .js,.ts,.vue src --fix",
    "lint:vue": "vue-tsc --noEmit --project tsconfig.vue.json",
    "test": "NODE_ENV=test npx tsx --test ./test/test_*.ts",
    "type": "yarn run lint && yarn run type-check",
    "type-check": "tsc --noEmit -p ./src/renderer/tsconfig.json"
  },
  "keywords": [],
  "author": {
    "name": "receptron team"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron-forge/plugin-vite": "^7.9.0",
    "@electron-forge/publisher-s3": "^7.9.0",
    "@electron/fuses": "^2.0.0",
    "@eslint/js": "^9.35.0",
    "@intlify/eslint-plugin-vue-i18n": "^4.1.0",
    "@types/clone-deep": "^4.0.4",
    "@types/deep-equal": "^1.0.4",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "bufferutil": "^4.0.9",
    "electron": "38.1.2",
